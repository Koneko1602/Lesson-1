#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Nikita/Desktop/Project/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@_98b653d73863705da47aac13e5c689c6/node_modules/ts-jest/node_modules:/mnt/c/Users/Nikita/Desktop/Project/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@_98b653d73863705da47aac13e5c689c6/node_modules:/mnt/c/Users/Nikita/Desktop/Project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Nikita/Desktop/Project/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@_98b653d73863705da47aac13e5c689c6/node_modules/ts-jest/node_modules:/mnt/c/Users/Nikita/Desktop/Project/node_modules/.pnpm/ts-jest@29.3.4_@babel+core@_98b653d73863705da47aac13e5c689c6/node_modules:/mnt/c/Users/Nikita/Desktop/Project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
